            <ITEXT CH=" "/>
            <para PARENT="main_code"/>
            <ITEXT CH="=$ rsync -a master/ slave/"/>
            <para PARENT="main_code"/>
            <ITEXT CH=" "/>
            <para PARENT="main_code"/>
            <ITEXT CH="=$ psql -p 4001 -d postgres -c &quot;select pg_stop_backup()&quot;"/>
            <para PARENT="main_code"/>
            <ITEXT CH="NOTICE:  pg_stop_backup complete, all required WAL segments have been archived"/>
            <para PARENT="main_code"/>
            <ITEXT CH=" pg_stop_backup"/>
