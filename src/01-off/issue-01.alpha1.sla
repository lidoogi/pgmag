            <para PARENT="main_code"/>
            <ITEXT CH="server stopped"/>
            <para PARENT="main_code"/>
            <ITEXT CH="=$ rsync -a slave/ slave2/"/>
            <para PARENT="main_code"/>
            <ITEXT CH="=$ rsync -a slave/ slave3/"/>
            <para PARENT="main_code"/>
            <ITEXT CH="=$ pg_ctl -D slave start"/>
            <para PARENT="main_code"/>
            <ITEXT CH="server starting"/>
            <para PARENT="main_code"/>
            <para PARENT="main_text"/>
            <ITEXT FONTSIZE="10" CH="Slave2 and 3 will be basically the same as slave, but with different port, and connecting to 4002 (slave) instead of 4001 (master) for their WAL. So, let’s do the changes:"/>
            <para PARENT="main_text"/>
            <para PARENT="main_text"/>
            <ITEXT CH="=$ perl -pi -e 's/port = 4002/port = 4003/' slave2/postgresql.co "/>
            <para PARENT="main_code"/>
            <ITEXT CH="=$ perl -pi -e 's/port = 4002/port = 4004/' slave3/postgresql.conf"/>
            <para PARENT="main_code"/>
            <ITEXT CH="=$ perl -pi -e 's/port=4001/port=4002/' slave{2,3}/recovery.conf"/>
            <para PARENT="main_code"/>
            <ITEXT CH="=$ perl -pi -e 's/slave.finish.recovery/slave2.finish.recovery/' slave2/recovery.conf"/>
            <para PARENT="main_code"/>
