            <para PARENT="main_code"/>
            <para PARENT="main_code"/>
            <ITEXT CH="Of course we need some tidying of the slave:"/>
            <para PARENT="main_text"/>
            <para PARENT="main_text"/>
            <ITEXT CH="=$ rm -f slave/pg_xlog/???????????????????????? slave/pg_xlog/archive_status/* slave/pg_log/* slave/postmaster.pid"/>
            <para PARENT="main_code"/>
            <ITEXT CH=" "/>
            <para PARENT="main_code"/>
            <ITEXT CH="=$ vim slave/postgresql.conf"/>
            <para PARENT="main_code"/>
            <ITEXT CH="    port = 4002"/>
            <para PARENT="main_code"/>
            <ITEXT CH="    hot_standby = on"/>
            <para PARENT="main_code"/>
            <para PARENT="main_text"/>
            <ITEXT FONTSIZE="10" CH="And I also create recovery.conf in slave/, with this content:"/>
            <para PARENT="main_text"/>
            <para PARENT="main_text"/>
            <ITEXT CH="restore_command = '/bin/false'"/>
            <para PARENT="main_code"/>
            <ITEXT CH="standby_mode = 'on'"/>
            <para PARENT="main_code"/>
            <ITEXT CH="primary_conninfo = 'port=4001 user=depesz'"/>
            <para PARENT="main_code"/>
            <ITEXT CH="trigger_file = '/tmp/slave.finish.recovery'"/>
            <para PARENT="main_code"/>
            <para PARENT="main_text"/>
            <ITEXT FONTSIZE="10" CH="And with this in place I can start slave:"/>
            <para PARENT="main_text"/>
            <para PARENT="main_text"/>
            <ITEXT CH="=$ pg_ctl -D slave start"/>
            <para PARENT="main_code"/>
            <ITEXT CH="server starting"/>
            <para PARENT="main_code"/>
            <ITEXT CH=" "/>
            <para PARENT="main_code"/>
            <ITEXT CH="=$ head -n 1 slave/postmaster.pid | xargs -IPG ps uwf -p PG --ppid PG"/>
